  "description": "Code to build and deploy for Lambda + API Gateway",
  "publishConfig": {
    "access": "public"
  },
  "main": "dist/index.js",
  "module": "dist/module/index.js",
  "directories": {
    "dist": "dist"
  },
  "repository": {
